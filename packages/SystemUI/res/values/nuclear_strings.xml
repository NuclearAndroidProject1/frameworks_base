    <string name="qs_equal_tile_title">Equal tiles</string>
    <string name="qs_equal_tile_summary"></string>
    <string name="qs_settings">Settings</string>
    <string name="qs_tile_columns_title">Columns</string>
    <string name="qs_tile_columns_three">3</string>
    <string name="qs_tile_columns_four">4</string>
    <string name="qs_tile_columns_five">5</string>
    <!-- 4G LTE switch -->
    <string name="show_fourg_title">Show 4G instead of LTE</string>
    <string name="show_fourg_summary">Replaces LTE statusbar indicator with 4G</string>
    <string name="statusbar_tuner_category_title">Hide items from statusbar</string>
